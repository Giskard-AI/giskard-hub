      "config:recommended",
      "helpers:pinGitHubActionDigests",
      ":pinOnlyDevDependencies"
   ],
   "packageRules":[
      {
         "groupName":"{{manager}} dependencies",
         "groupSlug":"{{manager}}-deps",
         "matchPackagePatterns":[
            "*"
         ],
         "matchUpdateTypes":[
            "*"
         ]
      }
   ]
}
