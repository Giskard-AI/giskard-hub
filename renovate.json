{
   "labels": [
      "dependencies"
   ],
   "$schema": "https://docs.renovatebot.com/renovate-schema.json",
   "extends": [
      "config:recommended",
      "helpers:pinGitHubActionDigests",
      ":pinOnlyDevDependencies"
   ],
   "ignoreDeps": [
      "sphinx-autodoc-typehints"
   ],
   "packageRules": [
      {
         "groupName": "{{manager}} dependencies",
         "groupSlug": "{{manager}}-deps",
         "matchUpdateTypes": [
            "*"
         ],
         "matchPackageNames": [
            "*"
         ]
      }
   ]
}
